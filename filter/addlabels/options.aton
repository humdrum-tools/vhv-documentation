@@BEGIN: OPTIONS
@TOOL: addkey

@@BEGIN: OPTION
@CATEGORY: 
@DEFINITION: d|default=s
@ARGUMENT: list
@SUMMARY: Set the default expansion list.
@@END: OPTION

@@BEGIN: OPTION
@CATEGORY: 
@DEFINITION: r|norep=s
@ARGUMENT: list
@SUMMARY: Set the <code>norep</code> expansion list ("no-repeat", meaning take second endings.)
@@END: OPTION

@@BEGIN: OPTION
@CATEGORY: 
@DEFINITION: l|labels
@ARGUMENT: list
@SUMMARY: Add labels in the specified measures.
@@END: OPTION

@@END: OPTIONS



